{
    "name": "tutfw/tutfw",
    "license": "MIT",
    "description": "Tutfw framework",
    "keywords": [
        "mongodb",
        "mysql",
        "mariadb",
        "doctrine",
        "dbal",
        "framework",
        "tutfw",
        "api",
        "microservice"
    ],
    "homepage": "https://github.com/mehdiable/tutfw",
